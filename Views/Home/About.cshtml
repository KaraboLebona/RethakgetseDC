                                  }
                                });

                                loading.style.display = 'none';

                                if (response.ok) {
                                  sentMessage.style.display = 'block';
                                  form.reset();
                                } else {
                                  const data = await response.json();
                                  errorMessage.innerText = data?.errors?.map(err => err.message).join(', ') || 'Oops! There was a problem.';
                                  errorMessage.style.display = 'block';
                                }
                              } catch (error) {
                                loading.style.display = 'none';
                                errorMessage.innerText = 'Oops! There was a problem.';
                                errorMessage.style.display = 'block';
                              }
                            });
                        </script>

                    </div><!-- End Contact Form -->

                </div>

